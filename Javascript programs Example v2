Example 34: Date and control statements
<!DOCTYPE html>
<html>
<head>
<meta charset = "utf-8">
<title>A First Program in JavaScript</title>
<script type="text/javascript">
var x=new Date();
var c;
function hours()
{
var h=x.getHours();
if(h>7)
{
c="<h1> the time is now "+h+"</h1>";
document.getElementById("Date").innerHTML=c;
}
}
function day()
{
if(x.getDay()==2)
{
c="Today we have lab";
c=typeof(x.getDay());
document.getElementById("Date").innerHTML=c;
}
else if (x.getDay()==3)
{
c="No class today";
document.getElementById("Date").innerHTML=c;
}
else if(x.getDay()==4)
{
c="Theory class";
document.getElementById("Date").innerHTML=c;
}
else
{
document.getElementById("Date").innerHTML="Invalid day";
}
}
function month()
{
c="<h1 style='color:magenta'>The current month is now "+x.getMonth()+"</h1>"
document.getElementById("Date").innerHTML=c;

}
function minutes()
{
c="<h1 style='color:orange'>The minute is noe "+x.getMinutes()+"</h1>"
document.getElementById("Date").innerHTML=c;
}
</script>
</head>
<body>
<input type="button" value="Print hour" onclick="hours()">
<input type="button" value="Print day" onclick="day()">
<input type="button" value="Print month" onclick="month()">
<input type="button" value="Print minutes" onclick="minutes()">
<h1>ghhgf</h1>
<p id="Date"></p>
</body>
</html>

