</p></div></body>
<script>
function start1()
{
alert("hello div");
}
function start2()
{
alert("hello para");
}
document.getElementById("output1").addEventListener("click",start1,false);
document.getElementById("output2").addEventListener("click",start2,true);
</script>
</html>
