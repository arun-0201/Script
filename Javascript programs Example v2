Example 31: Functions with tables
<!DOCTYPE html>
<html>
<head>
<meta charset = "utf-8"></meta>
<title>Switching between HTML5 List Formats</title>
<script type="text/javascript">
var c="";
function test()
{
evenodd();
sqrt1();
}
function evenodd()
{
var x=parseInt(window.prompt("enter a no"));
c=c+"<table border='4'><tr><td>Values are</td><td>Answer</td></tr>";
if(x%2==0)
c=c+"<tr><td>"+x+"</td><td>even no</td></tr>";
else
c=c+"<tr><td>"+x+"</td><td>odd no</td></tr>";
//document.getElementById("table").innerHTML=c;
}
function sqrt1()
{
var x=parseInt(window.prompt("enter a no"));
var y=Math.sqrt(x);
c=c+"<tr><td>sqrt of "+x+" is</td><td>"+y+"</td></tr>";
document.getElementById("table").innerHTML=c;
}
</script>
</head><body>
<input type="button" value="click me" onclick="test()">

<p id ="table"></p></body></html>
