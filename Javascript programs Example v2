Example 48 :Array methods (push , pop, reverse)
<!DOCTYPE html>
<html>
<head>
<meta charset = "utf-8">
<title>Using the do...while Repetition Statement</title>
<script>
var c=new Array(10,20,4,5,3,6);
var g=new Array(1,2,3,4,5,6);
var t=[10,20,,5,67,88];
var y=new Array(5,2);
document.write(y[0]);
document.write("<br>");
for(var i=0;i<c.length;i++)
{
document.write(c[i]+ " ");
}
document.write("<br>");
for(var i=0;i<t.length;i++)
{
if(t[i]==undefined)
{
t[i]=33;
}
document.write(t[i]+ " ");
}
document.write("<br>");
var x=[];
x.push(56,66,77,88);
document.write(x[2]);

var h;
h=x.pop();
document.write("The popped ele is" +h);
document.write("<br>");
//document.write(c.reverse() + "It is reversed");
arrayrev(g);
function arrayrev(g)
{
var arry=[];
for(var i=0;i<g.length;i++)
{
arry.push(g[i]);
document.write(arry[i]+"The pushed elements are"+"<br>");
}
document.write("<br>");
var revstr=[];
while(arry.length>0)
{
revstr=revstr+arry.pop();
}
for(var i=0;i<revstr.length;i++)
{
document.write(revstr[i]+"");} }
</script>
</head><body></body>
</html>
