<html>  
<head>  
<title> My page </title>  
<script type="text/javascript">  
function myFunc()  
{  
var Container=document.body.childNodes; 
for(i=0;i<Container.length;i++) 
{ 
/*if(Container[i].nodeName=='DIV') 
{ 
var btn = document.createElement("p"); 
var x=document.createTextNode("New hello"); 
var e=btn.appendChild(x);  
document.getElementById("div1").appendChild(e); 
} */ 
/*if(Container[i].nodeName=='UL') 
{ 
var x=document.getElementById("mylist1").lastElementChild.previousElementSibling; var btn=document.createElement("li"); 
btn.textContent="hellooG"; 
var x1=document.getElementById("mylist1"); 
x1.replaceChild(btn,x); 
}*/ 
if(Container[i].nodeName=='H1') 
{ 
var newPara = document.createElement("p"); 
// var newText = document.createTextNode("Hi i am new para!"); newPara.textContent="hi i am new";
//newPara.appendChild(newText); 
var firstPara = document.getElementById("head1").previousSibling; document.body.insertBefore(newPara, firstPara); 
firstPara.appendChild(newPara); 
} 
} 
} 
window.addEventListener("load",myFunc); 
</script>  
</head> 
