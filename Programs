document.write("The y is "+y[2]); 
document.write("<br>"); 
var z=y.split('fgh'); 
document.write("<br>"); 
document.write(typeof(z)); 
document.write("<br>"); 
document.write(z[2]);
document.write(fruits.join("fgh")); 
document.write("<br>"); 
//(0,1,2,3,4,5,6) 
