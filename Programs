Example 16: Mouse event 
<!DOCTYPE html> 
<html> 
<head> 
<title>String </title> 
<script> 
function start() 
{ 
document.getElementById("stringop").addEventListener("mousemove",mousemove1,false);  document.getElementById("stringop1").addEventListener("dblclick",mousemove2,false); } 
function mousemove1() 
{ 
var s="ZEBRAANIMAL"; 
var result=s.charAt(2); 
document.getElementById("output").innerHTML=result; 
} 
function mousemove2() 
{ 
var s="ZEBRAANIMAL"; 
var result=s.toLowerCase(); 
document.getElementById("output").innerHTML=result; 
} 
 window.addEventListener("load",start,false); 
</script> 
</head> 
<body> 
<input type ="button" id="stringop" value ="String operations "> 
 <input type ="button" id="stringop1" value ="String operations "> 
<p id ="output"> 
</body> 
</html>

<html>
<head>
<title> My page </title>
<script type="text/javascript">
function myFunc()
{

 var list = document.getElementsByTagName('div')[1].children;
 var list1 = document.getElementsByTagName('div')[1].childNodes;
 alert(list.length);

 for(var i=0;i<list.length;i++)
 {
 alert("The child of div is "+list[i]);
 }
   alert(list1.length);

 for(var i=0;i<list1.length;i++)
 {
 alert("The childnodes of div is "+list1[i]);
 }
 var x=document.body.children;
 document.write("The length is "+x.length +"<br>");
 for(var i=0;i<x.length;i++)
 {
 document.write("The ans is "+x[i] +"<br>");
 }
 }
</script>
</head>
<body>
<h1 id="title">This is main title</h1>
<p>hello hai</p>
<form name="FirstForm">
<input type="button" value="Click Me" onclick="myFunc();" />
<input type="text">
</form>
