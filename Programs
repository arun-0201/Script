function GetNodeInfo () { 
 var div = document.getElementById ("myDiv1"); 
var c1=div.firstChild; 
var c2=div.lastChild; 
var c3=div.firstElementChild; 
var c4=div.lastElementChild; 
var c5=div.firstChild.nextSibling; 
var c6=div.firstChild.nextElementSibling; 
var c7=div.lastChild.previousElementSibling; 
var c8=div.lastElementChild.previousSibling; 
alert("The first child is" +c1); 
alert("The last child is " +c2); 
alert("The firstElementChild is "+c3); 
alert("The lastElementChild is "+c4); 
alert("The firstChild nextSibling is"+c5); 
alert("The firstChild nextElementSibling is"+c6); 
alert("The lastChild previousSibling"+c7); 
alert("The lastElementChildChild previousSibling"+c8); 
 var commentNode1 = div.firstElementChild.nextElementSibling; var commentNode2 = div.firstChild.nextSibling;//.nextElementSibling; 
 //var message = ""; 
 alert (commentNode.nodeName); 
alert (commentNode.nodeType);
var list = document.getElementsByTagName('div')[1].childNodes; 
alert(list.length); 
} 
 </script> 
