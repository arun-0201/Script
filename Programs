<script type="text/javascript"> 
var name;  
var x = new Date();// current date and time 
var hour = x.getHours(); // current hour (0-23) 
name = window.prompt( "Please enter your name" ); if ( hour < 12 ) 
document.write( "<h1>Good Morning, " ); 
if ( hour >= 12 ){ 
hour = hour - 12; 
if ( hour < 6 ) 
document.write( "<h1>Good Afternoon, " ); 
if ( hour >= 6 ) 
document.write( "<h1>Good Evening, " );} 
document.write( name +", welcome to Js programming!</h1>" ); </script> 
