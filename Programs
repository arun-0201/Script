Date object 
<!DOCTYPE html> 
<html> 
<head> 
<meta charset="utf-8"> 
<title>sdf</title> 
<script> 
function start() 
{ 
var t=new Date(); 
document.write(t.toString()+"<br>"); 
document.write(t.toLocaleString()+"<br>"); 
document.write(t.toUTCString()+"<br>"); 
document.write(t.toString()+"<br>"); 
document.write(t.valueOf()+"<br>"); 
document.write(t.getTime()+"<br>");//Returns the time in ms from janu 1 1970 document.write(t.getHours()+"<br>"); 
document.write(t.getMinutes()+"<br>"); 
document.write(t.getUTCDay()+"<br>"); 
document.write(t.getTimezoneOffset()+"<br>"); 
document.write(t.getSeconds()+"<br>"); 
document.write(t.getMilliseconds()+"<br>"); 
document.write(t.getDate()+"<br>"); 
document.write(t.getDay()+"<br>"); 
document.write(t.getMonth()+"<br>"); 
document.write(t.getFullYear()+"<br>");
var newdate=new Date(); 
newdate.setHours(10,23,45); 
document.write(newdate.getHours()+"<br>"); document.write(newdate.getMinutes()+"<br>"); document.write(newdate.getSeconds()+"<br>"); 
} 
window.addEventListener("load",start,false); 
</script> 
<body> 
<section id = "newArguments"></section> <section id = "setMethods"></section> </body> 
</html> 
