$( "p" ).find( spans ).css( "color", "red" ); 
$( "ul li:last-child" ).css( "text-decoration", "underline" );
$( "ul li:first-child" ).css( "color", "green" ); $("li").siblings().css( "color", "magenta" ); $("li.gg").siblings().css( "color", "magenta" ); $( "#gt" ).before( "<h1>Hello</h1>" ); 
}); 
