Identify and count nodetypes 
<!DOCTYPE html> 
<html><head><script type="text/javascript"> 
function GetNodeInfo () 
 { 
var x=document.body.childNodes; 
var onetype=threetype=eighttype=0; 
for (i=0;i<x.length;i++) 
{ 
if(x[i].nodeType=="1") 
{ 
x[i].setAttribute("style","color:magenta"); 
onetype++; 
} 
if(x[i].nodeType=="3") 
{ 
threetype++; 
} 
if(x[i].nodeType=="8") 
{ 
eighttype++; 
} 
} 
var output=""; 
output+="element nodes are"+onetype+"text nodes are"+threetype+"comment nodes  are"+eighttype; 
document.getElementById("myDiv1").innerHTML=output; 
} 
</script>
</head> 
<body> 
 <div id="myDiv"> 
<h1>header1</h1> 
<p>para1</p> 
<h1>header2</h1> 
</div> 
<!--hello--> 
<h1>hello</h1> 
<div id="myDiv1"> 
<p>para2</p> 
</div> 
<!--hello--> 
<p>para</p> 
<ul id="test"> 
<li>dfg</li> 
<li>sdfd</ul> 
<!--hello--> 
 <button onclick="GetNodeInfo ()">Get node info!</button> </body> 
