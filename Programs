<head> 
<meta charset = "utf-8"> 
<title>Initializing an Array</title> 
</head> 
<script> 
function start() 
{ 
var array1 = [ [ 1, 2, 3 ],  
[ 4, 5, 6 ,7], 
[7,8,9 ,6,7,8,9], 
[9,10], 
[5,6]]; 
document.write("The length is "+array1[2].length+"<br>"); var total = 0; 
var rows = array1.length; 
for ( var row = 0; row < rows; ++row ) 
{  
document.write("The row is " +row+"<br>" );
 var columns = a[ row ].length;  
 for ( var col = 0; col < columns; ++col ) { 
document.write("The row is " +col+"<br>" ); total += a[ row ][ col ]; 
}  
} 
/* 
printArray(array1); 
document.write("<br>"); 
} 
function printArray(theArray) 
{ 
var sum1=0; 
for (var i=0; i<theArray.length;i++) { 
var no=theArray[i].length; 
for (var j=0;j<no;j++) 
{ 
theArray[2][i]=0; 
document.write(theArray[i][j]+" " ); } 
document.write("<br>" ); 
}*/ 
} 
window.addEventListener("load",start); </script> 
<body>
