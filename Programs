<title> My page </title>  
<script type="text/javascript">  
function myFunction() 
{ 
var test=document.body.childNodes; 
for(var i=0;i<test.length;i++) 
{ 
if(test[i].nodeName=='DIV') 
{ 
var paragraph=document.createElement("p"); 
var x=document.createTextNode("I have inserted new p tag"); 
var t=paragraph.appendChild(x); 
document.getElementById("div1").appendChild(t); 
} 
if(test[i].nodeName=='UL') 
{ 
var g=document.getElementById("mylist1").lastElementChild.previousElementSibling; var lis=document.createElement("li"); 
//document.write(typeof(lis)); 
lis.textContent="new list added"; 
var x1=document.getElementById("mylist1"); 
x1.replaceChild(lis,g); 
} 
if(test[i].nodeName=='H1') 
{ 
var paragraph1=document.createElement("p");
paragraph1.textContent="Hi i am a new para inserted"; var firsthead=document.getElementById("head1").nextSibling; document.body.insertBefore(paragraph1,firsthead); } 
} 
} 
window .addEventListener("load",myFunction); </script>  
