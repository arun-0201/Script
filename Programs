<!DOCTYPE html> 
<html> 
<head> 
<script type="text/javascript"> 
function CountOfNodes() 
{ 
var x=document.body.childNodes; var ones=threes=eights=0; 
for(var i=0;i<x.length;i++) 
{ 
if(x[i].nodeType==1) 
{ 
x[i].setAttribute("style","color:magenta"); ones++; 
} 
if(x[i].nodeType==3) 
{ 
threes++; 
} 
if(x[i].nodeType==8) 
{ 
eights++; 
} 
}
//var output=""; 
//document.write("The element nodes are "+ones +"<br>"); //document.write("The text Nodes are"+threes+"<br>"); //document.write("The comment nodes are"+eights+"<br>"); } 
</script> 
</head> 
<body> 
<div id="mydiv1"> 
<h1>head1</h1> 
<p>para1</p> 
<h3>head3</h3> 
</div> 
<!--Comment1--> 
<h1>header2</h1> 
<div id="myDiv2"> 
<p>para2</p> 
</div> 
<!--Comment2--> 
<p>sfsd</p> 
<ul id="df"> 
<li>sdfd</li> 
<li>sdfsdf</li> 
<li>sdf</li> 
</ul> 
<!---Comment3-->
