var count=0; 
while(foundpos!=-1) 
{ 
foundpos=mystr1.indexOf("script",foundpos); 
document.write("found at pos"+foundpos+"<br>"); 
if(foundpos!=-1) 
{ 
count++; 
foundpos++; 
} 
} 
document.write("There are "+count+"occurences in the string"); </script> 
